@model VolleyVerse.Models.Contatti
<link href="~/Content/css/Shop.css" rel="stylesheet" />
@{
    ViewBag.Title = "Informazioni e Contatti";
}

<div class="container mt-4">
    <div class="row">
        <div class="col-md-8">
            <h2>Informazioni e Contatti</h2>
            <hr />
            <h4> Contatti:</h4>
            <p><strong>Indirizzo:</strong> <span class="fw-light">@Model.address</span></p>
            <p><strong>Telefono:</strong> <span class="fw-light"> @Model.phone_number</span></p>
            <p><strong>Email:</strong> <span class="fw-light"> @Model.email</span></p>
            <h4>Info Biglietti:</h4>
            <p class="fw-light">Informazioni sui biglietti saranno disponibili presto.</p>
            <h4>Dove Trovarci:</h4>
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2903.3229944846953!2d10.521640753054294!3d43.307497541681265!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x12d5fdf90cc16d41%3A0x3f7ba28bab3fa0c8!2sA.S.D.%20Volley%20Cecina!5e0!3m2!1sit!2sit!4v1713298005726!5m2!1sit!2sit" width="500" height="350" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

        </div>
        <div class="col-md-4">

            <img src="https://volleybox.net/media/upload/teams/big_1693577869JU3ta.png" class=" volleyLogo img-responsive" alt="Immagine 1" />
            <img src="https://scontent.fpsa1-1.fna.fbcdn.net/v/t39.30808-6/436278674_948578217272244_8441626840017909555_n.jpg?_nc_cat=106&ccb=1-7&_nc_sid=5f2048&_nc_ohc=_GstGPVs5s0Ab6qltx7&_nc_ht=scontent.fpsa1-1.fna&oh=00_AfBuV14ZqyV7c9aX6x8DVJqYMzLv1reUbpPauyTvX7zFJg&oe=6624B982" class="volleyLogo img-responsive mb-2" alt="Immagine 2" />
            <img src="https://scontent.fpsa1-1.fna.fbcdn.net/v/t39.30808-6/436209140_947279307402135_8450819707168023662_n.jpg?_nc_cat=102&ccb=1-7&_nc_sid=5f2048&_nc_ohc=JJlPSz0csOcAb73hqeo&_nc_ht=scontent.fpsa1-1.fna&oh=00_AfAh04jaDDZ3Du_mt__G4XgtdqAW6Sl5isU8ONW4WBM97w&oe=6624C900" class="volleyLogo img-responsive mb-2" alt="Immagine 3" />
            <img src="https://scontent.fpsa1-2.fna.fbcdn.net/v/t39.30808-6/435675030_949408960522503_23705044519741638_n.jpg?_nc_cat=110&ccb=1-7&_nc_sid=5f2048&_nc_ohc=pLmQB7xO0n4Ab4hwC5j&_nc_ht=scontent.fpsa1-2.fna&oh=00_AfBq5KxKt_3R-QyMxg2cPnnQpMmri9Ac1m8csNu6z0j__Q&oe=6624D15D" class="volleyLogo img-responsive mb-2" alt="Immagine 4" />
            <img src="https://scontent.fpsa1-2.fna.fbcdn.net/v/t39.30808-6/436150307_948603357269730_7887161026396869498_n.jpg?_nc_cat=111&ccb=1-7&_nc_sid=5f2048&_nc_ohc=dmGmLzAN5O8Ab4Ans2q&_nc_ht=scontent.fpsa1-2.fna&oh=00_AfD_GV9lw-NPd685d494DY1-1seZb8VlGruT4qRgtEZHrw&oe=6624AEF2" class="volleyLogo img-responsive " alt="Immagine 5" />
        </div>
    </div>
    <div class="row">
        <h4 class="mb-3 mt-3">Seguici anche su </h4>
        <div class="d-flex align-items-center">
            <a class="facebook" href="https://www.facebook.com/"><img class="facebook2" src="https://upload.wikimedia.org/wikipedia/commons/c/cd/Facebook_logo_%28square%29.png" alt="Facebook" /><p class="social">Facebook</p></a>
            <a class="instragram" href="https://www.instagram.com/"><img class="instragram2" src="https://upload.wikimedia.org/wikipedia/commons/thumb/e/e7/Instagram_logo_2016.svg/2048px-Instagram_logo_2016.svg.png" alt="Instagram" /><p class="social">Instragram</p></a>
        </div>

    </div>
</div>
